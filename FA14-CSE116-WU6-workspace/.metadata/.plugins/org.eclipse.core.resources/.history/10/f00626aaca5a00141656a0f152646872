package code;

public class NonTaxable {

	private int _salePrice;

	public NonTaxable(int p) {
		_salePrice = p;
	}
	
	public int taxDue() {
		return 3000; // changed "return 0;" to "return 3000;" because this is the expected return value according to the test methods
	}
	
	public int price() {
		return _salePrice;
	}

}
